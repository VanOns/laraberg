{
    "name": "van-ons/laraberg",
    "description": "A Gutenberg implementation for Laravel",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Maurice Wijnia",
            "email": "maurice@van-ons.nl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/WordPress/gutenberg.git"
        }
    ],
    "require": {
        "embed/embed": "^3.3"
    },
    "require-dev": {
        "wordpress/gutenberg": "^8.5",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "VanOns\\Laraberg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VanOns\\Laraberg\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": "VanOns\\Laraberg\\LarabergServiceProvider"
        },
        "installer-paths": {
            "vendor/{$vendor}/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "npm install --prefix=vendor/wordpress/gutenberg",
            "npm run build --prefix=vendor/wordpress/gutenberg"
        ]
    },
    "config": {
        "process-timeout":0
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
